APP_NAME="Malaika Backend API"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=malaika_backend
# DB_USERNAME=root
# DB_PASSWORD=
# DB_FOREIGN_KEYS=true
# DB_URL=

# Cache Configuration
DB_CACHE_CONNECTION=
DB_CACHE_TABLE=cache
DB_CACHE_LOCK_CONNECTION=
DB_CACHE_LOCK_TABLE=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

# Storage Configuration
# Default filesystem disk (local, s3, gcs, azure, spaces)
FILESYSTEM_DISK=local

# Document Storage Configuration
DOCUMENTS_STORAGE_DRIVER=local
# DOCUMENTS_AWS_ACCESS_KEY_ID=
# DOCUMENTS_AWS_SECRET_ACCESS_KEY=
# DOCUMENTS_AWS_DEFAULT_REGION=us-east-1
# DOCUMENTS_AWS_BUCKET=
# DOCUMENTS_AWS_URL=
# DOCUMENTS_AWS_ENDPOINT=
# DOCUMENTS_AWS_USE_PATH_STYLE_ENDPOINT=false

# Avatar Storage Configuration
AVATARS_STORAGE_DRIVER=public
# AVATARS_AWS_ACCESS_KEY_ID=
# AVATARS_AWS_SECRET_ACCESS_KEY=
# AVATARS_AWS_DEFAULT_REGION=us-east-1
# AVATARS_AWS_BUCKET=
# AVATARS_AWS_URL=
# AVATARS_AWS_ENDPOINT=
# AVATARS_AWS_USE_PATH_STYLE_ENDPOINT=false

# Backup Storage Configuration
BACKUP_STORAGE_DRIVER=s3
# BACKUP_AWS_ACCESS_KEY_ID=
# BACKUP_AWS_SECRET_ACCESS_KEY=
# BACKUP_AWS_DEFAULT_REGION=us-east-1
# BACKUP_AWS_BUCKET=
# BACKUP_AWS_URL=
# BACKUP_AWS_ENDPOINT=
# BACKUP_AWS_USE_PATH_STYLE_ENDPOINT=false

CACHE_STORE=database
# CACHE_PREFIX=

# Memcached Configuration
MEMCACHED_HOST=127.0.0.1
MEMCACHED_PORT=11211
# MEMCACHED_PERSISTENT_ID=
# MEMCACHED_USERNAME=
# MEMCACHED_PASSWORD=

# Redis Configuration
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
# REDIS_CACHE_CONNECTION=cache
# REDIS_CACHE_LOCK_CONNECTION=default

# Email Configuration
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS Configuration
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false
# AWS_MALWARE_SCAN_BUCKET=

# Google Cloud Storage Configuration
# GOOGLE_CLOUD_PROJECT_ID=
# GOOGLE_CLOUD_KEY_FILE=
# GOOGLE_CLOUD_STORAGE_BUCKET=
# GOOGLE_CLOUD_STORAGE_PATH_PREFIX=
# GOOGLE_CLOUD_STORAGE_API_URI=

# Azure Blob Storage Configuration
# AZURE_STORAGE_NAME=
# AZURE_STORAGE_KEY=
# AZURE_STORAGE_CONTAINER=
# AZURE_STORAGE_URL=
# AZURE_STORAGE_PREFIX=

# DigitalOcean Spaces Configuration
# DO_SPACES_KEY=
# DO_SPACES_SECRET=
# DO_SPACES_ENDPOINT=
# DO_SPACES_REGION=
# DO_SPACES_BUCKET=
# DO_SPACES_URL=

# Pesapal Payment Gateway Configuration
# Get your credentials from https://developer.pesapal.com/
PESAPAL_ENVIRONMENT=sandbox
PESAPAL_CONSUMER_KEY=
PESAPAL_CONSUMER_SECRET=
PESAPAL_CALLBACK_URL="${APP_URL}/api/payments/pesapal/callback"
PESAPAL_IPN_URL="${APP_URL}/api/payments/pesapal/ipn"
PESAPAL_DEFAULT_NOTIFICATION_ID=
PESAPAL_DEFAULT_CURRENCY=KES
PESAPAL_ENABLE_CARD=true
PESAPAL_ENABLE_MOBILE_MONEY=true
PESAPAL_ENABLE_BANK_TRANSFER=true
PESAPAL_CONNECTION_TIMEOUT=30
PESAPAL_REQUEST_TIMEOUT=60
PESAPAL_MAX_RETRY_ATTEMPTS=3
PESAPAL_RETRY_DELAY=5
# PESAPAL_WEBHOOK_SECRET=
PESAPAL_LOG_REQUESTS=true
PESAPAL_LOG_RESPONSES=true

# Security Configuration
# Virus scanning method: basic, clamav, virustotal, aws_guardduty
VIRUS_SCAN_METHOD=basic
# CLAMAV_SOCKET=/var/run/clamav/clamd.ctl
# CLAMAV_TIMEOUT=30
# VIRUSTOTAL_API_KEY=
# VIRUSTOTAL_TIMEOUT=60

# File Upload Security
MAX_FILE_SIZE=10485760
MAX_FILES_PER_REQUEST=5
UPLOADS_PER_MINUTE=10
UPLOADS_PER_HOUR=100
UPLOADS_PER_DAY=500
SCAN_FILE_CONTENT=true
QUARANTINE_SUSPICIOUS_FILES=true

# Temporary File Cleanup
TEMP_CLEANUP_ENABLED=true
TEMP_MAX_AGE_HOURS=24
TEMP_CLEANUP_SCHEDULE=hourly

# SMS Services
# TWILIO_SID=
# TWILIO_TOKEN=
# TWILIO_FROM=
# AFRICASTALKING_USERNAME=
# AFRICASTALKING_API_KEY=
# AFRICASTALKING_FROM=

# Additional Services
# CLOUDINARY_CLOUD_NAME=
# CLOUDINARY_API_KEY=
# CLOUDINARY_API_SECRET=
# CLOUDINARY_SECURE=true

# Frontend URL for redirects
APP_FRONTEND_URL=http://localhost:3000

# Sanctum Configuration
SANCTUM_STATEFUL_DOMAINS=localhost,localhost:3000,127.0.0.1,127.0.0.1:8000,::1
SANCTUM_TOKEN_EXPIRATION=1440
SANCTUM_TOKEN_PREFIX=

# Authentication Configuration
# AUTH_GUARD=web
# AUTH_PASSWORD_BROKER=users
# AUTH_MODEL=App\Models\User
# AUTH_PASSWORD_RESET_TOKEN_TABLE=password_reset_tokens
# AUTH_PASSWORD_TIMEOUT=10800

VITE_APP_NAME="${APP_NAME}"
